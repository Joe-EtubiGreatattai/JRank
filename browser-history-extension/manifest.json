  {
    "manifest_version": 3,
    "name": "Browser History Extension",
    "version": "1.0",
    "permissions": [
      "history"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
      }
    ]
  }
  